     companyinfo.`pincode` AS companyinfo_pincode,
     companyinfo.`phone` AS companyinfo_phone,
     companyinfo.`tinNumber` AS companyinfo_tinNumber

FROM
     `journal` journal,
     `companyinfo` companyinfo
WHERE
